generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id_usuario   Int      @id @default(autoincrement())
  nome         String
  email        String   @unique
  senha        String
  tipo         String
  data_criacao DateTime @default(now())
  data_modificacao DateTime @default(now())
  status       String
}
